{
  "manifest_version": 2,

  "name": "VocabBuilder",
  "description": "IN DEVELOPMENT",
  "version": "1.0",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!",
    "persistent": false
  },

  "permissions": [
    "activeTab",
    "storage",
    "http://code.jquery.com/*",
    "https://ajax.googleapis.com/",
    "identity",
    "https://quizlet.com/*",
    "https://api.quizlet.com/*"
  ]
}